{
  "name": "@rinkkasatiainen/fraction-node",
  "version": "0.5.0",
  "description": "node module built for Worlds Best Introduction to TDD",
  "main": "index.js",
  "scripts": {
    "build": "yarn run tsc -p .",
    "test": "yarn run test:m; yarn run test:j",
    "test:m": "NODE_ENV=test ts-mocha test/**/**.spec.ts ",
    "test-watch:m": "NODE_ENV=test ts-mocha --watch --watch-files src/**/*.ts, test/**/*.spec.ts",
    "test:j": "NODE_ENV=test jest test/**/**.test.ts",
    "test:w": "npm run test -- --watch",
    "lint": "eslint -- src test",
    "lint:fix": "eslint --fix -- src test"
  },
  "files": [
    "lib/**/*"
  ],
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/expect": "^24.3.0",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.14",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "chai": "^4.3.7",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "react-a11y": "^1.1.0",
    "sinon": "^15.0.4",
    "sinon-chai": "^3.7.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.0.4",
    "ts-jest": "^29.1.0"
  },
  "dependencies": {}
}
